# Discord Bot Configuration
# ВАЖНО: НЕ ПУБЛИКУЙТЕ ЭТОТ ФАЙЛ С РЕАЛЬНЫМИ ТОКЕНАМИ!

# Discord Bot Token (получите на https://discord.com/developers/applications)
DISCORD_TOKEN=your_discord_bot_token_here

# Guild ID (ID вашего сервера Discord)
GUILD_ID=your_guild_id_here

# Admin User ID (ID главного администратора)
ADMIN_USER_ID=your_admin_user_id_here

# Database Configuration
DATABASE_PATH=club.db

# Bot Activity (статус бота)
BOT_ACTIVITY_NAME=SWAGA

# Commission rate for transfers (10%)
TRANSFER_COMMISSION_RATE=0.1

# Initial money for new users
INITIAL_MONEY=10

# Initial messages for new users
INITIAL_MESSAGES=1

# Role IDs for admin commands (замените на ваши)
ADMIN_ROLES=role_id_1,role_id_2,role_id_3,role_id_4

# Badge role IDs (замените на ваши)
BADGE_ROLES=1406251918489292963,1406252612843470969,1406255286573994026,1406283815449530408,1406283849096495266,1406284121902157854,1406284173785698334,1406284656839626872,1406284693326008373,1406284893352231072,1406284966186319923,1406285025359429825,1406285083190493294,1406690448365584425
# Contributor role ID (роль для контрибьюторов)
CONTRIBUTOR_ROLE_ID=your_contributor_role_id_here

# Tester role ID (роль для тестеров)
TESTER_ROLE_ID=your_tester_role_id_here

# Maintainer role ID (роль для мейнтейнеров)
MAINTAINER_ROLE_ID=your_maintainer_role_id_here

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Enable debug mode (true/false)
DEBUG_MODE=false

# Voice channel rewards
VOICE_TIME_REWARD=1
VOICE_MONEY_REWARD=20
VOICE_CHECK_INTERVAL=1

# Dynamic voice channels
DYNAMIC_VOICE_CATEGORY_ID=your_category_id_here
DYNAMIC_VOICE_LOBBY_ID=your_lobby_channel_id_here

# Spotify API Configuration (получите на https://developer.spotify.com/dashboard)
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here

# Music Player Settings
MUSIC_INACTIVITY_TIMEOUT=300
MUSIC_MAX_QUEUE_SIZE=100
MUSIC_DEFAULT_VOLUME=50