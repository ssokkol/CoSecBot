# Discord Bot Configuration
# ВАЖНО: НЕ ПУБЛИКУЙТЕ ЭТОТ ФАЙЛ С РЕАЛЬНЫМИ ТОКЕНАМИ!

# Discord Bot Token (получите на https://discord.com/developers/applications)
DISCORD_TOKEN=your_discord_bot_token_here

# Guild ID (ID вашего сервера Discord)
GUILD_ID=your_guild_id_here

# Admin User ID (ID главного администратора)
ADMIN_USER_ID=your_admin_user_id_here

# Database Configuration
DATABASE_PATH=club.db

# Bot Activity (статус бота)
BOT_ACTIVITY_NAME=ZOV ебёт азов

# Commission rate for transfers (10%)
TRANSFER_COMMISSION_RATE=0.1

# Ring price
RING_PRICE=100000

# Initial money for new users
INITIAL_MONEY=10

# Initial messages for new users
INITIAL_MESSAGES=1

# Role IDs for admin commands (замените на ваши)
ADMIN_ROLES=role_id_1,role_id_2,role_id_3,role_id_4

# Badge role IDs (замените на ваши)
BADGE_ROLES=badge_role_1,badge_role_2,badge_role_3,badge_role_4,badge_role_5,badge_role_6,badge_role_7,badge_role_8,badge_role_9,badge_role_10,badge_role_11,badge_role_12,badge_role_13,badge_role_14,badge_role_15,badge_role_16,badge_role_17,badge_role_18,badge_role_19,badge_role_20,badge_role_21,badge_role_22

# Contributor role ID (роль для контрибьюторов)
CONTRIBUTOR_ROLE_ID=your_contributor_role_id_here

# Tester role ID (роль для тестеров)
TESTER_ROLE_ID=your_tester_role_id_here

# Maintainer role ID (роль для мейнтейнеров)
MAINTAINER_ROLE_ID=your_maintainer_role_id_here

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Enable debug mode (true/false)
DEBUG_MODE=false

# Voice channel rewards
VOICE_TIME_REWARD=1
VOICE_MONEY_REWARD=20
VOICE_CHECK_INTERVAL=1
